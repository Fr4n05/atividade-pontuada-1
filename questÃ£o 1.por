programa {
  funcao inicio() {
     //Variaveis
     inteiro n1, n2, n3, soma
    
    // 
    escreva("Digite valor de A:")
    leia(n1)
    limpa()
    
    escreva("Digite valor de B:")
    leia(n2)
    limpa()

    escreva("Digite valor de C:")
    leia(n3)
    limpa()

    //calculos
    soma = n1+n2

    se (soma > n3)  
    escreva("Valor de C é maior")
   senao
   escreva("Valor de C é menor")
  }
}
